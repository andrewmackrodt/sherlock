{
  "name": "sherlock",
  "version": "0.0.0",
  "maintainers": [
    {
      "name": "Andrew Mackrodt",
      "url": "https://github.com/andrewmackrodt"
    }
  ],
  "scripts": {
    "coverage": "nyc mocha \"test/**/*Test.ts\"",
    "eslint": "eslint --ext .js,.ts src/ test/ types/",
    "eslint:fix": "eslint --ext .js,.ts --fix src/ test/ types/",
    "test": "mocha \"test/**/*Test.ts\""
  },
  "dependencies": {
    "dot-object": "^2.1.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/dot-object": "^1.7.0",
    "@types/lodash": "^4.14.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^12.12.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "acorn": "^7.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.0",
    "eslint-plugin-import": "^2.19.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.0",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
